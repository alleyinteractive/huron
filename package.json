{
  "name": "ai-prototype",
  "description": "An in-browser prototyping tool for Alley Interactive",
  "author": "Owen Stowe",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "start": "webpack --config src/config/webpack.config.js -d -w",
    "build": "PRODUCTION=true webpack --config src/config/webpack.config.js -p",
    "prepublish": "./node_modules/.bin/babel -d lib src/",
    "test": "./node_modules/.bin/babel-node ./src/huron.js --source './test-kss/**/*.scss' --destination 'partials/generated/'",
    "test-once": "./node_modules/.bin/babel-node ./src/huron.js --source './test-kss/**/*.scss' --destination 'partials/generated/' --runOnce",
    "help": "./node_modules/.bin/babel-node ./src/huron.js -h"
  },
  "bin": {
    "huron": "src/huron.js"
  },
  "main": "index.js",
  "dependencies": {
    "commander": "^2.9.0",
    "connect": "^3.4.0",
    "gaze": "^0.5.2",
    "jsdom": "^7.2.0",
    "kss": "^2.1.1",
    "serve-static": "^1.10.0",
    "webcomponents.js": "^0.7.19"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
    "jasmine-node": "^1.14.5",
    "rewire": "^2.5.1",
    "webpack": "^1.12.11"
  }
}
