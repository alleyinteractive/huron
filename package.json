{
  "name": "huron",
  "description": "An in-browser prototyping tool built on top of webpack and kss-node",
  "author": "Alley Interactive",
  "version": "3.0.0",
  "license": "GPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/alleyinteractive/huron"
  },
  "scripts": {
    "doc": "./node_modules/.bin/jsdoc src -r -c ./config/jsdoc.json",
    "help": "./node_modules/.bin/babel-node src/cli/index.js -h",
    "build": "npm run build:cli && npm run test:once && npm run build:web",
    "dev": "concurrently -k \"npm run dev:cli\" \"npm run dev:web\"",
    "dev:cli": "BABEL_ENV=cli webpack --config config/webpack.server.config.js --mode development",
    "build:cli": "BABEL_ENV=cli webpack --config config/webpack.server.config.js --mode production",
    "dev:web": "BABEL_ENV=browser webpack --config config/webpack.browser.config.js --mode development",
    "build:web": "BABEL_ENV=browser webpack --config config/webpack.browser.config.js --mode production",
    "test:once": "BABEL_ENV=test jest",
    "test": "BABEL_ENV=test jest --watch"
  },
  "bin": {
    "huron": "./dist/index.js"
  },
  "engines": {
    "node": "8",
    "npm": "6"
  },
  "files": [
    "dist",
    "config",
    "templates"
  ],
  "dependencies": {
    "babel-core": "^6.26.3",
    "chalk": "^2.4.1",
    "commander": "^2.15.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "dom-loader": "^1.0.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fs-extra": "^6.0.1",
    "gaze": "^1.1.2",
    "handlebars": "^4.0.11",
    "handlebars-loader": "^1.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "github:jantimon/html-webpack-plugin#webpack-4",
    "immutable": "^3.8.2",
    "json-loader": "^0.5.7",
    "kss": "^3.0.0-beta.20",
    "lodash": "^4.17.10",
    "opn": "^5.3.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.3",
    "webpack-dev-server": "^3.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "jest": "^22.4.3",
    "jsdoc": "^3.5.5",
    "mock-fs": "^4.5.0",
    "raw-loader": "^0.5.1",
    "regenerator-runtime": "^0.11.1",
    "webpack-cli": "^2.1.3",
    "webpack-node-externals": "^1.7.2"
  }
}
